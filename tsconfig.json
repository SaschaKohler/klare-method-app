{
  "compilerOptions": {
    "jsx": "react-native",
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "allowJs": true,
    "module": "ESNext",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true
  },
  "extends": "expo/tsconfig.base",
  "exclude": ["node_modules"]
}
